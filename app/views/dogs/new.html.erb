<div class="wrapper">
  <div class="container-form">
    <div class="row">
      <div class="col-md-8 col-md-offset-2">
        <h1>Rajoute ton clebard</h1>
        <%= simple_form_for @dog do |f| %>
          <%= f.input :name %>
          <%= f.input :age %>
          <%= f.input :address %>
          <%= f.input :breed %>
          <%= f.input :gender %>
          <%= f.input :description %>
          <%= f.input :photo %>
          <%= f.input :photo_cache, as: :hidden %>
          <%= f.button :submit, "J'ajoute mon chien", class: 'btn btn-warning' %>
        <% end %>
      </div>
    </div>
  </div>
</div>

